# CACHE GIT PASSWORD:
git config credential.helper store

# INSTALL APT-SELECT:
sudo pip install apt-select && sudo pip install --upgrade html5lib==1.0b8

sudo apt install screen
screen
screen ctrl + A, D
screen -r

# COMPLETE UNINSTALL
sudo apt-get purge --auto-remove <PACKAGE_NAME>

# Installed packages dump: conky

# FIND FILE: sudo find / -name "conky.*"

